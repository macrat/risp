" vim: set ft=lisp : "

(def assert ((import "./tests/test.risp") assert))

(do
  (def x 42)
  (assert = x 42))

(do
  (def x (+ 1 2))
  (assert = x 3))

(do
  (def x 1)
  (assert = x 1)
  (set x 2)
  (assert = x 2))

(do
  (def x "a")
  (do
    (def x "b")
    (assert = x "b"))
  (assert = x "a"))
