" Utilities for test "
" vim: set ft=lisp : "

(def assert (func (operator wanted actual)
  (if (not (operator wanted actual))
    (throw (list "assertion unsatisfied" (list operator wanted actual))))))

(assert = 1 1)

(try-catch (func (err)
                 (assert = err (list "assertion unsatisfied" (list = 1 2))))
           (assert = 1 2))
