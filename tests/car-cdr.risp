" vim: set ft=lisp : "

(def assert ((import "./tests/test.risp") assert))

(do "tests for list"
  (def xs (list 12 34 56))
  (assert = 12 (car xs))
  (assert = (list 34 56) (cdr xs))
  (assert = 34 (car (cdr xs))))

(do "tests for string"
  (def xs "hello")
  (assert = "h" (car xs))
  (assert = "e" (car (cdr xs)))
  (assert = "llo" (cdr (cdr xs))))
