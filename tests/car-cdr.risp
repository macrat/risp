" vim: set ft=lisp : "

(def test (import "./test.risp"))
(def assert (test assert))

((test case)
 "list"
 (func ()
       (def xs (list 12 34 56))
       (assert = 12 (car xs))
       (assert = (list 34 56) (cdr xs))
       (assert = 34 (car (cdr xs)))))

((test case)
 "tests for string"
 (func ()
       (def xs "hello")
       (assert = "h" (car xs))
       (assert = "e" (car (cdr xs)))
       (assert = "llo" (cdr (cdr xs)))))
